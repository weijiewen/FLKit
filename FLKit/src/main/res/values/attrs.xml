<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="cardBackgroundColor" format="color" />
    <attr name="cardElevation" format="dimension" />
    <attr name="cardCornerRadius" format="dimension" />
    <attr name="linearOrientation">
        <enum name="top_bottom" value="0" />
        <enum name="top_right_bottom_left" value="1" />
        <enum name="right_left" value="2" />
        <enum name="bottom_right_top_left" value="3" />
        <enum name="bottom_top" value="4" />
        <enum name="bottom_left_top_right" value="5" />
        <enum name="left_right" value="6" />
        <enum name="top_left_bottom_right" value="7" />
    </attr>
    <attr name="linearColors" format="string" />
    <declare-styleable name="FLTimeButton" >
        <attr name="cardBackgroundColor" />
        <attr name="cardElevation" />
        <attr name="cardCornerRadius" />
        <attr name="android:gravity" />
        <attr name="android:text" format="string" />
        <attr name="android:textSize" format="dimension" />
        <attr name="android:textColor" format="color" />
        <attr name="android:textColorHint" format="color" />
        <attr name="android:indeterminateTint" format="color" />
    </declare-styleable>
    <declare-styleable name="FLCardView">
        <attr name="cardElevation" />
        <attr name="cardCornerRadius" />
        <attr name="linearOrientation" />
        <attr name="linearColors" />
    </declare-styleable>
    <declare-styleable name="FLFlexLayout">
        <attr name="horizontalSpace" format="dimension"/>
        <attr name="verticalSpace" format="dimension"/>
    </declare-styleable>
</resources>